{
    "version": 2,
    "regions": [
        "hnd1"
    ],
    "env": {
        "PYTHONPATH": "/vercel/path0"
    },
    "builds": [
        {
            "src": "my_django/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "static"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "my_django/wsgi.py"
        }
    ]
}